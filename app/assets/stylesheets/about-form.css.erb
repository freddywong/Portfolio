#about-caption {
  text-shadow: none;
  font-family: lato, helvetica;
  color:#fff;  
  width: 800px;
  height: 600px;
  position:fixed;
  top: 72%;
  left: 50%;
  margin-top: -300px;
  margin-left: -350px;

}
.social-icons {
  width: 60px;
  margin-left: 10px;
  padding-top: 15px;
  padding-right: 15px;
  height: auto;
  float:left;
}

.about-paragraph{
  font-size: 1.5em;
  width:580px;
  float: left; 
  margin-left: 10px;
}
#third-paragraph {
  float:right;
}
.about-picture {
  height: 200px;
  width: auto;
  border-radius: 50%;
  float:left;
}

.about-project-btn {
  border-radius: 5px;  
  font-family: Arial;
  color: #ffffff;
  font-size: 29px;
  background: rgba(255,255,255,0);
  padding: 10px 20px 10px 20px;
  text-decoration: none;
  border: 3px solid white;
  float:right;
  margin-top: 10px;
  margin-left: 10px;
}

.about-project-btn:hover {
  text-decoration: none;
  color: #5e6668;
}